{
  "env": {},
  "globals": {},
  "parser": null,
  "parserOptions": {},
  "plugins": [
    "@getify/proper-arrows"
  ],
  "rules": {
    "@getify/proper-arrows/params": [
      "off"
    ],
    "@getify/proper-arrows/names": [
      "off"
    ],
    "@getify/proper-arrows/where": [
      "error",
      {
        "global": true,
        "property": true,
        "export": true,
        "trivial": true
      }
    ],
    "@getify/proper-arrows/return": [
      "error",
      {
        "object": true,
        "ternary": 0,
        "chained": true,
        "sequence": true,
        "trivial": false
      }
    ],
    "@getify/proper-arrows/this": [
      "error",
      "nested",
      {
        "no-global": true,
        "trivial": false
      }
    ]
  },
  "settings": {},
  "ignorePatterns": []
}
