{
  "env": {},
  "globals": {},
  "parser": "//node_modules/@babel/eslint-parser/lib/index.cjs",
  "parserOptions": {
    "requireConfigFile": false,
    "sourceType": "module"
  },
  "plugins": [
    "@babel"
  ],
  "rules": {
    "new-cap": [
      "off"
    ],
    "@babel/new-cap": [
      "error"
    ],
    "no-invalid-this": [
      "off"
    ],
    "@babel/no-invalid-this": [
      "warn"
    ],
    "no-unused-expressions": [
      "off"
    ],
    "@babel/no-unused-expressions": [
      "warn"
    ],
    "object-curly-spacing": [
      "off"
    ],
    "@babel/object-curly-spacing": [
      "error"
    ],
    "semi": [
      "off"
    ],
    "@babel/semi": [
      "error"
    ]
  },
  "settings": {},
  "ignorePatterns": []
}
