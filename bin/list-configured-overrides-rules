#!/usr/bin/env bash
set -euo pipefail

DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"
CONFIG=${1:-basic}
source ${DIR}/config.inc.sh

cd ${DIR}/..

eslint --no-eslintrc -c ${DIR}/../configs/.eslintrc.${CONFIG}.js --print-config foo.js | \
    jq -r ".rules | keys[]" | sort | { grep -v "^${PLUGIN_RULE_PREFIX}" || true; }
